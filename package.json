    "@playwright/test": "^1.56.0",
    "playwright": "^1.56.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "allure-playwright": "^2.15.1",
    "allure-commandline": "^2.29.0"
  }
}
